{
    "name": "week-1 repository check",
    "entry": "run-execution",
    "globals": {
        "cwd": "./week-1"
    }, "commands": [
        {
            "name": "check-directory",
            "command": "if [ -d './week-1' ]; then echo 'exists'; fi;",
            "cwd": ".",
            "expect": "exists"
        }, {
            "name": "check-files",
            "requires": [
                "check-directory"
            ],"command": [
                "if [ -f 'main.cc' ]; then echo 'exists'; fi;",
                "if [ -f 'a.cc' ]; then echo 'exists'; fi;",
                "if [ -f 'b.cc' ]; then echo 'exists'; fi;",
                "if [ -f 'a.h' ]; then echo 'exists'; fi;",
                "if [ -f 'b.h' ]; then echo 'exists'; fi;"
            ],
            "expect": [
                "exists", 
                "exists",
                "exists",
                "exists",
                "exists"
            ]
        }, {
            "name": "check-makefile",
            "requires": [
                "check-directory"
            ],
            "command": "if [ -f 'Makefile' ]; then echo 'exists'; fi;",
            "expect": "exists"
        }, {
            "name": "make-clean",
            "requires": [
                "check-files",
                "check-makefile"
            ],
            "command": "make clean",
            "failed": "make-manual-clean"
        }, {
            "name": "make-manual-clean",
            "requires": [
                "check-files",
                "check-makefile"
            ],
            "command": "rm *.o; rm main;"
        }, {
            "name": "make",
            "requires": ["make-clean"],
            "command": "make"
        }, {
            "name": "run-execution",
            "requires": ["make"],
            "command": "./main"
        }
    ]
}